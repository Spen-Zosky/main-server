    "@mui/x-data-grid": "^7.22.2",
    "@mui/x-date-pickers": "^8.9.0",
    "axios": "^1.7.7",
    "chart.js": "^4.4.6",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.6",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
